{
  "version": 2,
  "builds": [
    {
      "src": "dist/src/api.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "{node_modules/.prisma/client/**,node_modules/@prisma/engines/**,src/features/sales/prisma/generated/client/**}"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/dist/src/api.js" }
  ],
  "buildCommand": "npm run vercel-build"
}

{
  "version": 2,
  "builds": [
    { "src": "dist/src/api.js", "use": "@vercel/node" }
  ],
  "functions": {
    "dist/src/api.js": {
      "includeFiles": [
        "node_modules/.prisma/client/**",
        "node_modules/@prisma/engines/**"
      ]
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/dist/src/api.js" }
  ],
  "buildCommand": "npm run vercel-build"
}

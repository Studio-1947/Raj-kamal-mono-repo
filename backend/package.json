{
  "name": "@rk/backend",
  "version": "1.0.0",
  "description": "Backend API for Raj-Kamal web application",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "vercel-build": "rm -rf src/features/sales/prisma/generated && prisma generate && prisma generate --schema src/features/sales/prisma/schema.prisma && tsc",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "seed:admin": "tsx src/scripts/seed-admin.ts",
    "seed:online": "tsx src/features/sales/scripts/import-excel-events.ts --only online",
    "seed:online:file": "tsx src/features/sales/scripts/import-excel-events.ts --only online --file",
    "seed:online:backfill": "tsx src/features/sales/scripts/backfill-online.ts",
    "seed:events:preview": "tsx src/features/sales/scripts/preview-events.ts",
    "db:truncate:events": "tsx src/features/sales/scripts/truncate-events.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "postinstall": "prisma generate && prisma generate --schema src/features/sales/prisma/schema.prisma"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prisma": "^5.7.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "api",
    "backend",
    "express",
    "typescript",
    "prisma"
  ],
  "author": "Raj-Kamal Team",
  "license": "MIT"
}
